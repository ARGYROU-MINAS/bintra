{
  "name": "bintra",
  "version": "0.1.0",
  "description": "Binary Transparency Directory API",
  "main": "app.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node app.js",
    "test": "mocha --reporter mochawesome --exit",
    "sona": "mocha test --reporter mocha-sonarqube-reporter --reporter-options output=test-results.xml --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-summary mocha --exit",
    "testlink": "mocha --reporter=xunit --exit",
    "adduser": "node local/adduser.js",
    "addadmin": "node local/addadmin.js"
  },
  "keywords": [
    "swagger",
    "binary transparency"
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cdigit": "^2.6.0",
    "connect": "^3.2.0",
    "custom-env": "^2.0.1",
    "datejs": "^1.0.0-rc3",
    "dotenv": "^8.2.0",
    "events": "^3.2.0",
    "feed": "^4.2.2",
    "js-yaml": "^3.3.0",
    "json-patch": "^0.7.0",
    "jsonwebtoken": "^8.5.1",
    "mastodon": "^1.2.2",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.15",
    "oas3-tools": "^2.2.2",
    "piwik-tracker": "^1.1.2",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-json-pattern": "^1.1.0",
    "chai-json-schema": "^1.5.1",
    "chai-string": "^1.5.0",
    "jsdoc": "~3.6.6",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mochawesome": "^6.2.1",
    "nyc": "^15.1.0",
    "supertest": "^6.0.1"
  }
}
