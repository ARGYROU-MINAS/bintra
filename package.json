{
  "name": "bintra",
  "version": "1.4.27",
  "description": "Binary Transparency Directory API",
  "main": "app.js",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "prestart": "npm ci",
    "start": "node app.js",
    "test": "npx mocha --reporter mochawesome --timeout 10000 --exit",
    "sona": "mocha test --reporter mocha-sonarqube-reporter --reporter-options output=test-results.xml --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-summary mocha --exit",
    "combined": "nyc --reporter=lcov --reporter=text-summary mocha --timeout 20000 --reporter mocha-multi-reporters --reporter-options configFile=testreport-config.json --exit",
    "testlink": "mocha --reporter=testlinkreporter --exit",
    "adduser": "node local/adduser.js",
    "addadmin": "node local/addadmin.js",
    "changepwd": "node local/changepwd.js",
    "actuser": "node local/actuser.js",
    "deluser": "node local/deluser.js"
  },
  "keywords": [
    "swagger",
    "binary transparency"
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@sentry/tracing": "^6.19.3",
    "@sentry/node": "^6.19.4",
    "ansi-regex": "^6.0.1",
    "bcrypt": "^5.0.1",
    "cdigit": "^3.1.3",
    "connect": "^3.2.0",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "datejs": "^1.0.0-rc3",
    "dotenv": "^16.0.0",
    "events": "^3.2.0",
    "feed": "^4.2.2",
    "hpp": "^0.2.3",
    "js-yaml": "^4.1.0",
    "json-patch": "^0.7.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.4",
    "mastodon": "^1.2.2",
    "matomo-tracker": "^2.2.4",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.9",
    "mqtt": "^4.3.7",
    "node-resque": "^9.1.6",
    "oas3-tools": "^2.2.2",
    "prom-client": "^14.0.0",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.15.0",
    "swagger-ui-express": "^4.1.6",
    "toobusy-js": "^0.5.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@jsdevtools/chai-exec": "^2.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-json-pattern": "^1.1.0",
    "chai-json-schema": "^1.5.1",
    "chai-string": "^1.5.0",
    "jsdoc": "~3.6.9",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mochawesome": "^3.1.2",
    "nyc": "^15.1.0",
    "superagent": "^7.1.2",
    "supertest": "^6.2.1",
    "testlinkreporter": "git+https://gitlab.kretschmann.software/kai/testlinkimporter.git#1.0.5"
  }
}
