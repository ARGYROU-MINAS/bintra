{
  "name": "bintra",
  "version": "1.5.0",
  "description": "Binary Transparency Directory API",
  "main": "app.js",
  "homepage": "https://bintra.directory/",
  "bugs": {
    "url": "https://gitlab.kretschmann.software/kgroup/bintra/-/issues"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "prestart": "npm ci",
    "start": "node app.js",
    "test": "npx mocha --reporter mochawesome",
    "lint": "eslint './**/*.js'",
    "lintfix": "eslint --fix './**/*.js'",
    "sona": "mocha test --reporter mocha-sonarqube-reporter --reporter-options output=test-results.xml",
    "coverage": "nyc --reporter=lcov --reporter=text-summary mocha",
    "combined": "nyc --reporter=lcov --reporter=text-summary mocha --reporter mocha-multi-reporters --reporter-options configFile=testreport-config.json",
    "testlink": "mocha --reporter=testlinkreporter",
    "adduser": "node local/adduser.js",
    "addadmin": "node local/addadmin.js",
    "changepwd": "node local/changepwd.js",
    "actuser": "node local/actuser.js",
    "deluser": "node local/deluser.js",
    "sbom": "cyclonedx-node --include-dev --include-license-text --type application --output bom.json"
  },
  "keywords": [
    "swagger",
    "binary transparency"
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@cyclonedx/bom": "^3.10.4",
    "@sentry/node": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "ansi-regex": "^6.0.1",
    "bcrypt": "^5.0.1",
    "cdigit": "^3.1.3",
    "connect": "^3.2.0",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "datejs": "^1.0.0-rc3",
    "dotenv": "^16.0.2",
    "events": "^3.2.0",
    "feed": "^4.2.2",
    "hpp": "^0.2.3",
    "js-yaml": "^4.1.0",
    "json-patch": "^0.7.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.6.1",
    "mastodon": "^1.2.2",
    "matomo-tracker": "^2.2.4",
    "mongodb": "^4.9.1",
    "mongoose": "^6.5.4",
    "mqtt": "^4.3.7",
    "node-resque": "^9.2.0",
    "oas3-tools": "^2.2.2",
    "prom-client": "^14.1.0",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.15.0",
    "swagger-ui-express": "^4.5.0",
    "toobusy-js": "^0.5.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@jsdevtools/chai-exec": "^2.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-json-pattern": "^1.1.0",
    "chai-json-schema": "^1.5.1",
    "chai-string": "^1.5.0",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jsdoc": "~3.6.11",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "superagent": "^8.0.0",
    "supertest": "^6.2.4",
    "testlinkreporter": "git+https://gitlab.kretschmann.software/kai/testlinkimporter.git#1.0.5"
  }
}
